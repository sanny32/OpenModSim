#!/usr/bin/make -f

%:
	dh $@

override_dh_builddeb:
	dh_builddeb -- -Zgzip

override_dh_dwz:
	:

override_dh_shlibdeps:
	dh_shlibdeps \
		--dpkg-shlibdeps-params="--ignore-missing-info" \
		-- \
		--ignore-missing-info \
		-l@QT_LIB_PATH@

	sed -i '/shlibs:Depends=/ s/libgcc-s1 ([^)]*)//g' debian/*.substvars
	sed -i '/shlibs:Depends=/ s/,,/,/g' debian/*.substvars
	sed -i '/shlibs:Depends=/ s/, /, /g' debian/*.substvars
	sed -i '/shlibs:Depends=/ s/,\s*$//' debian/*.substvars

override_dh_strip:
	dh_strip --no-automatic-dbgsym