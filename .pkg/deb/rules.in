#!/usr/bin/make -f

%:
	dh $@

override_dh_builddeb:
	dh_builddeb -- -Zgzip

override_dh_dwz:
	:

override_dh_shlibdeps:
	dh_shlibdeps \
		--dpkg-shlibdeps-params="--ignore-missing-info" \
		-l@QT_LIB_PATH@

	sed -i '/libgcc-s1/d' debian/@PACKAGE_NAME@.substvars

	echo "---- substvars BEFORE ----"
	ls debian/*.substvars || true
	cat debian/*.substvars || true

	sed -i '/libgcc-s1/d' debian/@PACKAGE_NAME@.substvars

	echo "---- substvars AFTER ----"
	cat debian/*.substvars || true

override_dh_strip:
	dh_strip --no-automatic-dbgsym